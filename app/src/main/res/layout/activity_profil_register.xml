<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/liste_rappel"
    tools:context=".Profil.ProfilRegisterActivity">

    <Button
        android:id="@+id/btn_creation"
        android:layout_width="168dp"
        android:layout_height="77dp"
        android:layout_marginTop="60dp"
        android:layout_marginBottom="96dp"
        android:text="@string/btn_creationcompte"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tps_suppl"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="539dp"
        android:layout_height="239dp"
        android:layout_marginBottom="180dp"
        android:rotation="19"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.588"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/img_rappelnuage"
        android:contentDescription="@string/label_img"/>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="325dp"
        android:layout_height="41dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="56dp"
        android:text="@string/txt_mtnmtn"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <EditText
        android:id="@+id/tps_prepa"
        android:layout_width="168dp"
        android:layout_height="49dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="44dp"
        android:hint="@string/txt_tps_prepa"
        android:inputType="number"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <EditText
        android:id="@+id/tps_suppl"
        android:layout_width="168dp"
        android:layout_height="49dp"
        android:layout_marginTop="36dp"
        android:text="@string/txt_tps_prepa"
        android:inputType="number"
        android:textColor="@color/white"
        android:hint="@string/txt_tps_prepa"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.345"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="319dp"
        android:layout_height="46dp"
        android:layout_marginTop="48dp"
        android:text="@string/txt_profiltusersarien"
        android:textColor="@color/white"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.413"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tps_prepa" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="335dp"
        android:layout_height="38dp"
        android:layout_marginTop="48dp"
        android:text="@string/txt_bvnjpp"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="340dp"
        android:layout_height="69dp"
        android:layout_marginTop="16dp"
        android:text="@string/txt_sltjsuitonpote"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="180dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tps_suppl"
        app:srcCompat="@drawable/ic_accueilbas"
        android:contentDescription="@string/label_img" />

    <ImageView
        android:id="@+id/imageView24"
        android:layout_width="80dp"
        android:layout_height="65dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.767"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:layout_constraintVertical_bias="0.332"
        app:srcCompat="@drawable/img_chapeaudiplome"
        android:contentDescription="@string/label_img"/>

    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="326dp"
        android:layout_height="92dp"
        android:contentDescription="@string/label_img"
        app:layout_constraintBottom_toTopOf="@+id/btn_creation"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.367"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:layout_constraintVertical_bias="0.415"
        app:srcCompat="@drawable/ic_rectangle" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="326dp"
        android:layout_height="92dp"
        android:layout_marginTop="19dp"
        android:layout_marginBottom="30dp"
        android:contentDescription="@string/label_img"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.367"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:srcCompat="@drawable/ic_rectangle" />

</androidx.constraintlayout.widget.ConstraintLayout>